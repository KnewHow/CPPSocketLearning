{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "server debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/socket_test",
            "args": [
                "--gtest_filter=Customer.INSERT"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
